        <title>Writing Modifier</title>
        <author>
            <name>2artisans</name>
        </author>
        <link href="https://2artisans.github.io/mycompass-alpha/audiobook-creator-4.html"/>
        <id>https://2artisans.github.io/mycompass-alpha/audiobook-creator-4.html</id>
            <category term="Writing Tools"/>
            <category term="Toolbox"/>

        <updated>2025-01-16T18:30:33-05:00</updated>
            <summary></summary>
        <content type="html">
            <![CDATA[
                <iframe loading="lazy" width="100%" id="dynamic-iframe" src="https://web.hydn.cc/askdaisy/programs/writing_modifier" frameborder="0" data-responsive="false"></iframe>
<p>
<script>
    document.addEventListener("DOMContentLoaded", function() {
    function adjustIframeHeight() {
        const iframe = document.getElementById("dynamic-iframe");
        if (iframe) {
            const viewportHeight = window.innerHeight;
            const iframeOffsetTop = iframe.offsetTop;
            const calculatedHeight = viewportHeight - iframeOffsetTop;
            iframe.style.height = `${calculatedHeight}px`;
        }
    }

    // Adjust on load
    adjustIframeHeight();

    // Adjust on window resize
    window.addEventListener("resize", adjustIframeHeight);
});
         </script>
</p>
            ]]>
        </content>
